            <? comment.returnTags.forEach(function(returnTag) { -?>
              <?= /*' ' + returnTag.joinedTypes + */' ' + returnTag.description ?>
            <? }) ?>
          <? } ?>
        <? } else { ?>
          ## <?= comment.name ?>
        <? } ?>
      <? } ?>

        <?= comment.description ?>

        <? if (comment.deprecated) { ?>
